<style>
	.sectionFooter {
		width: 100%;
		height: 30vh;
		position: relative;
	}

	.footer-content{
		display: flex;
		flex-direction: column;
		justify-content: space-around;
		align-items: center;
		background: linear-gradient(0deg, #F7F7F766 30%, #33333300 100%);
		width: 100%;
		height: 100%;
	}

	.footer-reserved {
			font-size: 1.3rem;
			width: 50%;
			text-align: center;
		}

	.footer-content-reserved {
		display: flex;		
		justify-content: space-evenly;
		align-items: center;
		width: 100%;
	}

	@media screen and (max-width: 768px){
		.footer-content-reserved {
			flex-direction: column;
		}	
		.footer-reserved {
			font-size: 0.8rem;
		}			
		.footer-content-socialmedia {
			margin-top: 20px;
		}
	}

	.footer-content-socialmedia {
		display: flex;
		justify-content: space-evenly;
		align-items: center;

		width: 50%;
		/* height: 100%; */
		/* background: linear-gradient(0deg, rgba(5,202,192,0.4) 30%, rgba(255, 255, 255, 0) 100%);   */
		
		/* background-color: #f7f7f7c0; */
		/* border: 1px solid #777;

		border-radius: 20px; */
		
	}
	.hr_custom {
		top: 0;
		height: 3px; /* Equivalente a h-[2px] en Tailwind */
		width: 90%; /* Equivalente a w-full en Tailwind */
		min-width: 18rem; /* Equivalente a min-w-[18rem] en Tailwind */
		border-top: 0; /* Equivalente a border-t-0 en Tailwind */
		background: transparent; /* Equivalente a bg-transparent en Tailwind */
		background-image: linear-gradient(to right, transparent, #78866b, transparent); /* Equivalente a bg-gradient-to-r from-transparent via-white to-transparent en Tailwind */
		background-position: center;
	}
	a {
		width: 20px;
		height: 20px;		

		border-radius: 50%;
	}

	@media ( max-width:  768px){

		.sectionFooter {
			height: 35vh;
		}
		a {
			width: 15px;
			height: 15px;			
		}
    }
	a svg {
		/* cambiar color del svg */
		fill: #f7f7f7c0;
	}
</style>
